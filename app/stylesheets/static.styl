@import 'dependencies'
@import 'config'

.p-static
  width: $width
  margin: 0 auto
  padding: 30px 10px 60px
  box-sizing: border-box

  h2
    color: $blue
    font-size: 36px
    line-height: 48px

  p
    font-size: 16px
    line-height: 24px

  @media $mobile
    min-width: 0
    padding: 0 5px

