{
  "name": "expressjs-boilerplate",
  "description": "ExpressJS boilerplate with SSR React SPA frontend",
  "license": "MIT",
  "readmeFilename": "README.md",
  "homepage": "http://max.degterev.me/",
  "author": "Max Degterev <max@degterev.me>",
  "repository": "suprMax/expressjs-boilerplate",
  "bugs": "https://github.com/suprMax/expressjs-boilerplate/issues",
  "private": true,
  "version": "0.0.1",
  "main": "app.js",
  "engines": {
    "node": "^10.x.x"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install && npm run decache",
    "check": "npm test && npm run lint && echo \"\\033[0;32mAll checks pass\\033[0m\"",
    "gulp": "gulp",
    "start": "gulp",
    "production": "NODE_ENV=production SANDBOX=true gulp build && NODE_ENV=production SANDBOX=true node app.js",
    "decache": "find ./build -type f -name '.browserify-cache-*.json' -delete || echo \"\\033[0;32mAlready decached!\\033[0m\"",
    "lint": "gulp lint || true",
    "test": "TZ='GMT' mocha \"test/**/*.{js,es}\"",
    "test:watch": "TZ='GMT' mocha -w \"test/**/*.{js,es}\"",
    "test:tofile": "TZ='GMT' mocha --no-colors -R spec \"test/**/*.{js,es}\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "browserify-incremental": "^3.1.1",
    "chai": "^4.2.0",
    "connect-livereload": "^0.6.1",
    "errorhandler": "^1.5.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
